<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Sample Site</title>
     <link rel='stylesheet' href='/stylesheets/style.css' />
    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.1/css/bootstrap.min.css"/>
   <script src="https://code.jquery.com/jquery-2.2.0.min.js"></script>
</head>
<body>
  <div class="container">
    <!-- Static navbar -->
    <nav class="navbar navbar-default" style="background-color: #2b3f40;">
      <div class="container-fluid">
        <div id="navbar" class="navbar-collapse collapse">
          <ul class="nav navbar-nav">
            <li><a  style="color: aliceblue;" href="/listUsers">Users List</a></li>
          </ul>
          <ul class="nav navbar-nav navbar-right">
            <li><a style="color: aliceblue;" href="/logout">Logout</a></li>
          </ul>
        </div><!--/.nav-collapse -->
      </div><!--/.container-fluid -->
    </nav>
  <div class="row">
      <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6 col-xs-offset-0 col-sm-offset-0 col-md-offset-3 col-lg-offset-3 toppad" >
  
        <div class="panel panel-info">
          <div class="panel-heading" style="background-color: #102925;">
            <h3 class="panel-title" style="color: aliceblue"><%=data[0].first_name%> <%=data[0].last_name%></h3>
          </div>
          <div class="panel-body">
            <div class="row">
              <div class="col-md-3 col-lg-3 "> <img alt="User Pic" src="<%=data[0].image %>" class="img-circle img-responsive"> </div>
              <div class=" col-md-9 col-lg-9 "> 
                <table class="table table-user-information">
                  <tbody>
                    <tr>
                      <td>User Name:</td>
                      <td><%=data[0].user_name%></td>
                    </tr>
                    <tr>
                      <td>Password:</td>
                      <td><%=data[0].password%></td>
                    </tr>
                    <tr>
                      <td>Country:</td>
                      <td><%=data[0].country%></td>
                    </tr>
                    <tr>
                      <td>Job Title:</td>
                      <td><%=data[0].job%></td>
                    </tr>
                    <tr>
                      <td>Email:</td>
                      <td><%=data[0].email%></td>
                    </tr>
                  </tbody>
                </table>
              </div>
            </div>
          </div>
      </div>

  </div>
</body>
</html>